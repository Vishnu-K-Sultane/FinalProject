@model FinalProject.UI.AmbulanceDto

@{
    ViewData["Title"] = "Ambulance Details";
}

<h3>Ambulance Details</h3>

<div>
    <h4>Ambulance</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Ambulance ID</dt>
        <dd class="col-sm-10">@Model.AmbulanceId</dd>

        <dt class="col-sm-2">Vehicle Number</dt>
        <dd class="col-sm-10">@Model.VehicleNumber</dd>

        <dt class="col-sm-2">Vehicle Type</dt>
        <dd class="col-sm-10">@Model.VehicleType</dd>

        <dt class="col-sm-2">Driver Name</dt>
        <dd class="col-sm-10">@Model.DriverName</dd>

        <dt class="col-sm-2">Mobile Number</dt>
        <dd class="col-sm-10">@Model.MobileNumber</dd>
    </dl>
</div>

