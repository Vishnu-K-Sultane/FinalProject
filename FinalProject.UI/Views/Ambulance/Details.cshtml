@model FinalProject.UI.Models.Ambulance

<h2>Ambulance Details</h2>

<div>
    <strong>Vehicle Number:</strong> @Model.VehicleNumber
</div>
<div>
    <strong>Vehicle Type:</strong> @Model.VehicleType
</div>
<div>
    <strong>Driver Name:</strong> @Model.DriverName
</div>
<div>
    <strong>Mobile Number:</strong> @Model.MobileNumber
</div>

<a asp-action="Edit" asp-route-id="@Model.AmbulanceId" class="btn btn-warning">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back</a>
